    "type": "git",
    "url": "git+https://github.com/itsjonq/stylis-plugin-css-variables.git"
  },
  "bugs": {
    "url": "https://github.com/itsjonq/stylis-plugin-css-variables/issues"
  },
  "homepage": "https://github.com/itsjonq/stylis-plugin-css-variables#readme",
  "keywords": [],
